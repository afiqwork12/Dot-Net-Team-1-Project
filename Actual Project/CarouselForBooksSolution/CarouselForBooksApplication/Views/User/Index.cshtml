@model IEnumerable<CarouselForBooksApplication.Models.User>

<table class="table table-striped">
    <thead>
        <tr>
            @*<th>
                Id
            </th>*@
            <th>
                Name
            </th>
            <th>
                Username
            </th>
            <th>
                Email
            </th>
            <th>
                Password
            </th>
            <th>
                Dob
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            @*<td>
                @item.Id
            </td>*@
            <td>
                @item.Name
            </td>
            <td>
                @item.Username
            </td>
            <td>
                @item.Email
            </td>
            <td>
                @item.Password
            </td>
            <td>
                @item.Dob
            </td>
        </tr>
        }
    </tbody>
</table>
